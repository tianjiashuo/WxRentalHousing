page {
  background-color: #fff;
  font-size: medium;
  margin-top: 10rpx;
}

#pics {
 
  width: 100%;
}
#pic{
  width: 100%;
}
.tip {
  background-color: #fff3cd;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 14px;
  color: #fb6951;
  padding-left: 10rpx;
}
.tip text:nth-of-type(1) {
  margin-right: 10rpx;
}
view #type{
  position: relative;
  left: 30rpx;
  top : 30rpx;
  height: 90rpx;
  margin-bottom: 30rpx;
  border-bottom: 1px solid #eeeeee;  
}
 view select{
  position:absolute;
  right: 60rpx;
  top: 0rpx;
}
form view {
  padding: 20rpx;
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #eeeeee;
}
 /* form view:nth-last-of-type(1){
  border: none;
}  */
 
form view label {
  margin-right: 30rpx;
}

form view input {
  padding-top: 20rpx;
  line-height: 80rpx;
}

form view switch{
  position: absolute;
  right: 15rpx;
}
.tip-msg{
  color: #a4a4a4;
}
.tip-msg text{
  margin-left: 10rpx;
  font-size: 12px;
}
.uppic{
  margin-left: 10rpx;
  margin-top: 10rpx;
  width: 100rpx;
  height: 100rpx;
  border: 1px dashed #a4a4a4;
  display: flex;
  justify-content: center;
  align-items: center;
}

.sendbtn{
  position: fixed;
  bottom: 0px;
  width: 100%;
  height: 100rpx;
  background:#fff;
  padding: 0;
  margin: 0;
  text-align: center;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index:9999;
  font-size: 16px;
}
.sendbtn button {
  width: 50%;
  height: 100rpx;
  opacity: 1;
  /* top: 0; */
  /* border-radius: 15rpx; */
}
#submit{
  color: #fff;
  background-color: #008899;
}
#cancel{
color: #fff;
background-color: #c8c2be;
}
.imgbox{
  height: 200rpx;
  display: flex;
  justify-content: space-between
}
.imgbox image{
  width: 160rpx;
  height: 160rpx;
}
.mask{
  width:100%;
  height:100%;
  position:fixed;
  background-color:#999;
  z-index:9999;
  top:0;
  left:0;
  opacity:0.5;
}

#genderlabel{
  display: flex;
  flex-direction: row;
  position: relative;
  align-items:center;
}