  text-align: center;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  /* z-index:9999; */
  font-size: 16px;
  border-radius: 8%;
}
.pass{
  margin:0;
  background-color: #83ccd2;
  border-radius: 29px;

}
.refuse{
  margin:0;
