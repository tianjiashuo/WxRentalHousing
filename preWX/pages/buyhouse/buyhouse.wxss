/* pages/buyhouse/buyhouse.wxss */
/* pages/first/first.wxss */

.swiper {
  width: 100%;
  height: 350rpx;
}

.qwe {
  width: 100%;
}

.item {
  position: relative;
}

.asd {
  width: 40rpx;
  position: absolute;
  z-index: 99;
  top: 70rpx;
  left: 40rpx;
}

.small {
  position: absolute;
  color: white;
  font-size: 12px;
  top: 150rpx;
  left: 40rpx;
}

.big {
  position: absolute;
  color: white;
  font-size: 20px;
  top: 190rpx;
  left: 40rpx;
}

.none {
  height: 300rpx;
  width: 100%;
}

form input{
  height: 60rpx;
  padding: 10rpx;
  margin: 20rpx;
}

.search {
  position: absolute;
  top: 300rpx;
  width: 90%;
  height: 300rpx;
  left: 5%;
  border-radius: 6rpx;
  background-color: white;
  border: 1px solid #ecebe7;
  border-bottom:  3px solid #ecebe7;
}
.searchborder{
  border-bottom: 1px solid #ecebe7;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.content1 {
  border-top: 3px solid #ecebe7;
  margin: 30rpx;
  border-bottom: 1px solid #ecebe7 ;
}
.city {
  height: 48rpx;
  padding-bottom: 20rpx;
  /* line-height: rpx; */
  color: #656565;
  font-weight:550
}
.gps{
  position: absolute;
  left: 80rpx;
  top:30rpx;
  width:2rem;
  height: 2rem;
  display: flex;
}

.date {
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  line-height: 100rpx;
}

.date view:first-child {
  color: #008489;
}

.date view:last-child {
  color: #008489;
}

#id {
  width: inherit;
}

button {
  color: white;
  background-color: #758a99;
  font-size: 16px;
}

.autumn {
  width: 90%;
  margin: 0 auto;
  margin-top: 10rpx;
}

.word {
  font-size: 22px;
}

.infor {
  font-size: 12px;
}

.scroll {
  height: 104rpx;
  margin-top: 30rpx;
  display: flex;
  margin-bottom: 30rpx;
}

.block {
  width: 140rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 12px;
  text-align: center;
  border: 1px solid #ecebe7;
  margin-right: 20rpx;
  flex-shrink: 0;
  border-radius: 4px;
}

.change {
  width: 140rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 12px;
  text-align: center;
  border: 1px solid #ecebe7;
  margin-right: 20rpx;
  flex-shrink: 0;
  border-radius: 4px;
  background-color: #758a99;
  color: white;
}

.house {
  display: flex;
  flex-wrap: wrap;
  /* margin-top: 30rpx; */
  justify-content: space-between;
  margin-top:30rpx;
  border-bottom:3px solid #95949a ;
}

.pichouse {
  width: 48%;
}

.pichouse image {
  width: 100%;
  margin-bottom: 10rpx;
}

.adress {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  font-size: 14px;
}

.type {
  font-size: 10px;

}

.price {
  display: flex;
  margin-bottom: 30rpx;
  height: 40rpx;
  margin-top: 10rpx;
}

.price view:nth-child(1) {
  font-size: 14px;
  line-height: 40rpx;
  margin-right: 10rpx;
}
.price view:nth-child(2) {
  font-size: 12px;
  line-height: 40rpx;
  margin-right: 10rpx;
}

.price view:nth-child(3) {
  font-size: 12px;
  line-height: 40rpx;
  margin-left: 30%;
}

.price view:nth-child(4) {
  line-height: 40rpx;
  font-size: 12px;
}



.page{
  width: 90%;
  margin: 0 auto;
  border: 1px solid #ecebe7;
  margin-top: 40rpx;
  border-radius: 6rpx;
}
.moldel{
  display: flex;
  margin: 50rpx 40rpx;
  border-bottom: 1px solid #ecebe7;
}
.testa{
  margin-left: 20rpx;
}
.testa text:nth-child(1){
  font-size: 14px;
  font-weight: 900;
}
.testa text:nth-child(2){
  font-size: 14px
}

